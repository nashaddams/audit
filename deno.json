{
  "name": "@nashaddams/audit",
  "version": "0.5.3",
  "license": "MIT",
  "exports": "./mod.ts",
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@deno/gfm": "jsr:@deno/gfm@0.10.0",
    "@octokit/plugin-rest-endpoint-methods": "npm:@octokit/plugin-rest-endpoint-methods@13.2.6",
    "@std/cli/unstable-spinner": "jsr:@std/cli@1.0.9/unstable-spinner",
    "@std/collections/intersect": "jsr:@std/collections@1.0.9/intersect",
    "@std/collections/partition": "jsr:@std/collections@1.0.9/partition",
    "@std/assert": "jsr:@std/assert@1.0.10",
    "@std/testing/bdd": "jsr:@std/testing@1.0.8/bdd",
    "@std/testing/mock": "jsr:@std/testing@1.0.8/mock",
    "@std/testing/snapshot": "jsr:@std/testing@1.0.8/snapshot"
  },
  "tasks": {
    "build": "deno compile -RW=. -E=NO_COLOR,FORCE_COLOR,TERM -N=api.jsr.io,api.github.com,0.0.0.0 --allow-run=\"npm\" -o dist/audit mod.ts",
    "audit": "deno run -RW=. -E=NO_COLOR,FORCE_COLOR,TERM -N=api.jsr.io,api.github.com --allow-run=\"npm\" mod.ts",
    "report": "deno run -R=. -E=NO_COLOR,FORCE_COLOR,TERM -N=0.0.0.0 mod.ts report",
    "test": "deno test -RW -N=api.jsr.io,api.github.com --coverage=cov",
    "coverage:lcov": "deno coverage --exclude=test --lcov cov > cov.lcov",
    "coverage:html": "deno coverage --html --exclude=test cov",
    "coverage:console": "deno coverage --exclude=test cov",
    "coverage": "deno task coverage:lcov && deno task coverage:html && deno task coverage:console",
    "doc": "deno doc --html mod.ts && deno doc --lint mod.ts"
  },
  "publish": {
    "include": [
      "mod.ts",
      "src/**/*.ts",
      "LICENSE",
      "README.md"
    ]
  },
  "lock": false
}
